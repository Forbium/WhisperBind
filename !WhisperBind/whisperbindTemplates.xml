 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
 
	<EditBox name = "editboxTemplate" inherits = "InputBoxTemplate" virtual="true" autoFocus = "false">
		<Size>
			<AbsDimension x = "60" y = "24"/>
